{
  "name": "radio-gestion",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:backend": "npm --prefix backend-radio run dev",
    "dev:frontend": "npm --prefix frontend-radio start",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "prod:backend": "npm --prefix backend-radio start",
    "prod:frontend": "npm --prefix frontend-radio run serve",
    "prod": "concurrently \"npm run prod:backend\" \"npm run prod:frontend\""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-select": "^5.7.3",
    "react-toastify": "^9.1.3",
    "recharts": "^3.1.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss-animate": "^1.0.7"
  }
}
